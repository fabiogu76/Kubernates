pipeline {
 agent any
    tools {
        jdk 'Java17'
    }
  stages {
    stage('Checkout') {
      steps {
        sh 'echo passed'
        //git branch: 'main', url: 'https://github.com/iam-veeramalla/Jenkins-Zero-To-Hero.git'
      }
    }
	stage('Build and Test') {
      steps {
        sh 'ls -ltr'
        // build the project and create a JAR file
        sh 'cd admin-service && mvn clean  install -Dmaven.test.skip=true '
      }
    }
  }
}